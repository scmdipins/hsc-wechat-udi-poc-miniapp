.wraper {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.view-box {
  width: 91.2vw !important;
  margin: 1vh 0;
  padding: 1.5vh 0;
  border-radius: 4rpx;
  background-color: #1c2d66;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  transform: rotate(-360deg);
  font-family: PingFangSC;
  font-size: 32rpx;
  font-weight: normal;
  color: #ffffff;
}

.active {
  background-color: #1474a4;
}

.img-manager {
  width: 6.4vw;
  height: 6.4vw;
  background: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: contain;
  margin-right: 6.4vw;
}