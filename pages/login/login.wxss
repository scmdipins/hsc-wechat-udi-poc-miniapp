.wrapper {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: radial-gradient(circle at 50% 50%, rgba(255, 255, 255, 0.3), rgba(255, 255, 255, 0) 87%);
  background-color: #1474a4;
}

.header {
  width: 100vw;
  height: 43.3vh;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.logo {
  width: 100%;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.img-logo {
  width: 75px;
  height: 97px;
  background: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: contain;
}

.title {
  height: 12.65%;
  font-family: PingFangSC;
  font-size: 24px;
  font-weight: 500;
  color: #ffffff;
}

.footer {
  width: 100vw;
  height: 56.7vh;
  padding: 0 4.4vw;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.auth-box {
  width: 91.2vw !important;
  height: 6.63vh;
  margin-top: 9.12vh;
  border-radius: 4rpx;
  background-color: #1c2d66;
}

.auth-content {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  transform: rotate(-360deg);
  font-family: PingFangSC;
  font-size: 32rpx;
  font-weight: normal;
  color: #ffffff;
}

.img-wechat {
  width: 6.4vw;
  height: 6.4vw;
  background: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: contain;
  margin-right: 6.4vw;
}

.btn-login {
  width: 100%;
  height: 7.63vh;
  display: flex;
  justify-content: center;
  align-items: center;
  transform: rotate(-360deg);
  font-family: CentraleSans;
  font-size: 16px;
  font-weight: normal;
  color: rgba(255, 255, 255, 0.85);
}
